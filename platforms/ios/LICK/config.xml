<?xml version='1.0' encoding='utf-8'?>
<widget id="com.meltcreative.LICK" version="0.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <splash height="480" src="app/ios/splash/Default~iphone.png" width="320" />
    <splash height="960" src="app/ios/splash/Default@2x~iphone.png" width="640" />
    <splash height="1024" src="app/ios/splash/Default-Portrait~ipad.png" width="768" />
    <splash height="2048" src="app/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
    <splash height="768" src="app/ios/splash/Default-Landscape~ipad.png" width="1024" />
    <splash height="1536" src="app/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
    <splash height="1136" src="app/ios/splash/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" src="app/ios/splash/Default-667h.png" width="750" />
    <splash height="2208" src="app/ios/splash/Default-736h.png" width="1242" />
    <splash height="1242" src="app/ios/splash/Default-Landscape-736h.png" width="2208" />
    <icon height="180" src="app/ios/icon/icon-60@3x.png" width="180" />
    <icon height="60" src="app/ios/icon/icon-60.png" width="60" />
    <icon height="120" src="app/ios/icon/icon-60@2x.png" width="120" />
    <icon height="76" src="app/ios/icon/icon-76.png" width="76" />
    <icon height="152" src="app/ios/icon/icon-76@2x.png" width="152" />
    <icon height="40" src="app/ios/icon/icon-40.png" width="40" />
    <icon height="80" src="app/ios/icon/icon-40@2x.png" width="80" />
    <icon height="57" src="app/ios/icon/icon.png" width="57" />
    <icon height="114" src="app/ios/icon/icon@2x.png" width="114" />
    <icon height="72" src="app/ios/icon/icon-72.png" width="72" />
    <icon height="144" src="app/ios/icon/icon-72@2x.png" width="144" />
    <icon height="29" src="app/ios/icon/icon-small.png" width="29" />
    <icon height="58" src="app/ios/icon/icon-small@2x.png" width="58" />
    <icon height="50" src="app/ios/icon/icon-50.png" width="50" />
    <icon height="100" src="app/ios/icon/icon-50@2x.png" width="100" />
    <name>LICK</name>
    <description>
        Notes so simple, your tongue can use it.
    </description>
    <author email="e@ject.ch" href="http://ject.ch">
        Melt Creative Industries
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <icon src="app/icon/apple-touch-icon.png" />
</widget>
